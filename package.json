{
  "name": "tango-search",
  "description": "A Vue.js project",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules --optimize-minimize",
    "test": "xo && ava --verbose"
  },
  "dependencies": {
    "algoliasearch": "^3.27.1",
    "vue": "^2.5.16",
    "vue-instantsearch": "^1.7.0",
    "vue-observe-visibility": "^0.4.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "ava": "^0.25.0",
    "axios": "^0.18.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-3": "^6.24.1",
    "cheerio": "^0.22.0",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.4",
    "lodash": "^4.17.5",
    "superstruct": "^0.5.1",
    "voca": "^1.4.0",
    "vue-loader": "^15.2.4",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.3",
    "webpack-dev-server": "^3.1.4",
    "xo": "^0.21.1"
  },
  "xo": {
    "space": true,
    "semicolon": false,
    "ignore": [
      "**/dist/**"
    ],
    "envs": [
      "node",
      "browser"
    ]
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit",
    "snapshotDir": "snapshots"
  }
}
